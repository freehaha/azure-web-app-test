{
  "trailingSlash": "auto",
  "routes": [],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "script-src 'self';object-src 'none';base-uri 'none';require-trusted-types-for 'script';",
    "x-frame-options": "DENY",
    "strict-transport-security": "max-age=63072000"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
